PREFIX data: <http://stats.data-gov.ie/data/>
PREFIX qb: <http://purl.org/linked-data/cube#>
PREFIX dimension: <http://purl.org/linked-data/sdmx/2009/dimension#>
PREFIX property: <http://stats.data-gov.ie/property/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX stats: <http://stats.data-gov.ie/codelist/geo/>

SELECT ?supcon1label ?supcon2label  ?prop ?proplabel ?geo ?geolabel ?val 
WHERE { 
GRAPH <http://data-gov.ie/graph/ssi> {
?obs a qb:Observation .
?obs dimension:refPeriod <http://reference.data.gov.uk/id/year/2009> .
?obs property:geoArea ?geo .
?geo skos:prefLabel ?geolabel .
?obs ?prop ?val .
?prop a qb:MeasureProperty .
?prop rdfs:label ?proplabel .
?prop qb:concept ?concept .
?concept skos:inScheme stats:service-indicators-2009 .
?concept skos:broader ?supcon1 .
?supcon1 skos:broader ?supcon2 .
?supcon1 skos:altLabel ?supcon1label .
?supcon2 skos:altLabel ?supcon2label .
} } order by  ?supcon1label
